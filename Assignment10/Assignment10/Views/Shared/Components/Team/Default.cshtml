@model IEnumerable<Team>

<a asp-action="Index" asp-controller="Home" asp-route-teamid="">Home</a>
<br />
<br />


@foreach (var x in Model)
{
    <a asp-action="Index"
       asp-controller="Home"
       asp-route-teamid="@x.TeamId"
       asp-route-teamname="@x.TeamName">@x.TeamName</a>
    <br />
    <br />
}